<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="3.9" client="site" method="upgrade">
	<name>Module - Open Source</name>
	<author>Alexon Balangue</author>
	<creationDate>30 january 2017</creationDate>
	<copyright>Copyright (C) 2012-2020 AlexonbStudio. All Right Reserved.</copyright>
	<license>http://www.gnu.org/licenses/gpl-3.0.html GNU/GPL</license>
	<authorEmail>contact@alexonbstudio.fr</authorEmail>
	<authorUrl>www.alexonbstudio.fr</authorUrl>
	<version>2.4.1</version>
	<description>MOD_OPENSOURCE_XML_DESC</description>
	<media destination="mod_opensource" folder="media">
		<filename>index.html</filename>
		<folder>assets</folder>	
	</media>
	<files>
		<filename module="mod_opensource">mod_opensource.php</filename>
		<filename>mod_opensource.xml</filename>
		<folder>tmpl</folder>
		<filename>index.html</filename>		
	</files>
	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.mod_opensource.ini</language>
		<language tag="fr-FR">fr-FR/fr-FR.mod_opensource.ini</language>
	</languages>
	
	<administration>
		<languages folder="language">
			<language tag="en-GB">en-GB/en-GB.mod_opensource.sys.ini</language>
			<language tag="fr-FR">fr-FR/fr-FR.mod_opensource.sys.ini</language>
		</languages>
	</administration>

	<config>
		<fields name="params">
			<fieldset name="basic">
				<field 
					name="html4or5"
					 type="radio" 
					default="1"
					label="HTML"
					description="JFIELD_MOD_OPENSOURCE_HTML">
						<option value="0">HTML4/4.1</option>
						<option value="1">HTML5</option>
				</field>
				<field 
					name="dnsprefetch_custom"
					type="text" 
					default=""
					label="JFIELD_MOD_OPENSOURCE_CDNCUSTOM_LABEL"
					description="JFIELD_MOD_OPENSOURCE_CDNCUSTOM_DESC"/>
				<field 
					type="spacer" 
					label="JFIELD_MOD_OPENSOURCE_MOREINFORMATION"
					description="Version" />				
			</fieldset>
			<fieldset name="plugin_lxw_site" label="Configuration">		
				<field 
					name="fontawesome-site"
					 type="list" 
					default="0"
					label="fontawesome"
					description="">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">Local fontawesme v5</option>
						<option value="2">CDN fontawesme free v4</option>
				</field>
				<field 
					name="jquery-site"
					 type="list" 
					default="0"
					label="Jquery"
					description="Version 1.12.4/2.2.4/3.3.1">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">MaxCDN minified v1.x</option>
						<option value="2">MaxCDN minified v2.x</option>
						<option value="3">MaxCDN minified v3.x</option>
						<option value="4">Google API minified v1.x</option>
						<option value="5">Google API minified v2.x</option>
						<option value="6">Google API minified v3.x</option>
						<option value="7">Cloudflare CDN minified v1.x</option>
						<option value="8">Cloudflare CDN minified v2.x</option>
						<option value="9">Cloudflare CDN minified v3.x</option>
						<option value="10">Jsdelivr CDN minified v1.x</option>
						<option value="11">Jsdelivr CDN minified v2.x</option>
						<option value="12">Jsdelivr CDN minified v3.x</option>
				</field>
				<field 
					name="modernizr-site"
					 type="radio" 
					default="0"
					label="Modernizr"
					description="Version 2.8.3">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="2">Cloudflare CDN minified</option>
				</field>
				<field 
					name="jqueryeasingjs-site"
					 type="radio" 
					default="0"
					label="jquery.easing.js"
					description="Version 1.4.1">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">CDN Cloudflare minifield</option>
				</field>	
				<field 
					name="bootstrap-site"
					 type="list" 
					default="0"
					label="Bootstrap"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">MaxCDN minified v3.3.7</option>
						<option value="2">minified CDN CloudFlare v4</option>
				</field>	
				<field 
					name="cookiesEU-site"
					 type="list" 
					default="0"
					label="Cookies (E.U.)"
					description="Jquery Cookie Version 1.4.1">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">minified (Expert CDN jsdelivr)</option>
						<option value="2">minified (Expert CDN CloudFlare)</option>
						<option value="3">minified (Novice CDN CloudFlare)</option>
						<option value="4">minified (Novice CDN CloudFlare/CSS Own Custom)</option>
						<option value="5">minified (Novice CDN jsdelivr)</option>
						<option value="6">minified (Novice CDN jsdelivr/CSS Own Custom)</option>
						<option value="7">minified (expert js/CSS Own Custom)</option>
				</field>	
			</fieldset>
			<fieldset name="effect-styles" label="Effect/effet">
				<field 
					name="animate-site"
					 type="list" 
					default="0"
					label="Animate.css"
					description="Version 3.5.2">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">jsdelivr CDN minified</option>
						<option value="2">CloudFlare CDN minified</option>
				</field>
				<field 
					name="wowjs-site"
					 type="list" 
					default="0"
					label="wow.js"
					description="Version 1.1.2">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">jsdelivr CDN minified</option>
						<option value="2">CloudFlare CDN minified</option>
				</field>
				<field 
					name="countupjs-site"
					 type="list" 
					default="0"
					label="CountUp"
					description="Version 1.9.3">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">CloudFlare CDN minified Anguler</option>
						<option value="2">CloudFlare CDN minified (default)</option>
				</field>
				<field 
					name="waypointsjs-site"
					 type="list" 
					default="0"
					label="waypoints"
					description="Version 4.0.1">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">CloudFlare CDN minified (Jquery)</option>
				</field>
				<field 
					name="parallaxjs-site"
					 type="list" 
					default="0"
					label="Parallax"
					description="Version 3.1.0">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="2">CloudFlare CDN minified (Jquery)</option>
						<option value="2">CloudFlare CDN minified (default)</option>
				</field>
			</fieldset>
			<fieldset name="advanced-pre" label="Prerender and prefetch support">
				<field 
					type="spacer" 
					label="prerender"
					description="Version" />
				<field 
					name="prerender-url"
					type="text" 
					default=""
					label="URL"
					description="Version" />
				<field 
					type="spacer" 
					label="prefetch"
					description="Version" />
				<field 
					name="prefetch-url"
					type="media" 
					default=""
					label="Image/CSS"
					description="JFIELD_MOD_OPENSOURCE_PREFETCH_DESC"
					directory="" />
				<field 
					type="spacer" 
					label="dns-prefetch"
					description="Version" />	
				<field 
					name="dnsprefetch_yoursite"
					type="radio" 
					default="0"
					label="JFIELD_MOD_OPENSOURCE_MYSITE"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="dnsprefetch_googleapi"
					type="radio" 
					default="0"
					label="Google Ajax API Labs"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="dnsprefetch_bootstrapcdn"
					type="radio" 
					default="0"
					label="BootStrap MaxCDN"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="dnsprefetch_jquery"
					type="radio" 
					default="0"
					label="JQuery"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="dnsprefetch_cdnjscloudflare"
					type="radio" 
					default="0"
					label="CloudFlare CDN JS"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="dnsprefetch_aspnetcdn"
					type="radio" 
					default="0"
					label="ASPNet CDN"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
			</fieldset>
		</fields>
	</config>
</extension>
