<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="4.0" client="site" method="upgrade">
	<name>Module - Open Source</name>
	<author>Alexon Balangue</author>
	<creationDate>30 january 2017</creationDate>
	<copyright>Copyright (C) 2012-2020 AlexonbStudio. All Right Reserved.</copyright>
	<license>http://www.gnu.org/licenses/gpl-3.0.html GNU/GPL</license>
	<authorEmail>contact@alexonbstudio.fr</authorEmail>
	<authorUrl>www.alexonbstudio.fr</authorUrl>
	<version>2.4.3</version>
	<description>MOD_OPENSOURCE_XML_DESC</description>
	<media destination="mod_opensource" folder="media">
		<filename>index.html</filename>
		<folder>assets</folder>	
	</media>
	<files>
		<filename module="mod_opensource">mod_opensource.php</filename>
		<filename>mod_opensource.xml</filename>
		<folder>tmpl</folder>
		<filename>index.html</filename>		
	</files>
	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.mod_opensource.ini</language>
		<language tag="en-GB">en-GB/en-GB.mod_opensource.sys.ini</language>
		<language tag="fr-FR">fr-FR/fr-FR.mod_opensource.ini</language>
		<language tag="fr-FR">fr-FR/fr-FR.mod_opensource.sys.ini</language>
	</languages>

	<config>
		<fields name="params">
			<fieldset name="basic">
				<field 
					type="spacer" 
					label="JFIELD_MOD_OPENSOURCE_MOREINFORMATION"
					description="Version" />
				<field 
					name="dnsprefetch_yoursite"
					type="list" 
					default="1"
					label="JFIELD_MOD_OPENSOURCE_MYSITE"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="dnsprefetch_googleapi"
					type="list" 
					default="0"
					label="Google Ajax API Labs"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="dnsprefetch_cdnjscloudflare"
					type="list" 
					default="0"
					label="CloudFlare CDN JS"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>	
				<field 
					name="dnsprefetch_custom"
					 type="text"
					default=""
					label="DNS prefetch" 
					description="eg: https://cdn.domain.tld"
					filter="string" 
				/>		
				<field 
					name="prefetch_url"
					 type="text"
					default=""
					label="prefetch" 
					description="eg: https://domain.tld"
					filter="string" 
				/>			
			</fieldset>
			<fieldset name="sources" label="Sources">		
				<field 
					name="fontawesome-site"
					 type="list" 
					default="0"
					label="font awesome"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="jquery-site"
					 type="list" 
					default="0"
					label="Jquery"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="jqueryeasingjs-site"
					 type="list" 
					default="0"
					label="jquery.easing.js"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>	
				<field 
					name="bootstrap-site"
					 type="list" 
					default="0"
					label="Bootstrap"
					description="bundle Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>					
				<field 
					name="animate-site"
					 type="list" 
					default="0"
					label="Animate.css"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="wowjs-site"
					 type="list" 
					default="0"
					label="wow.js"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="countupjs-site"
					 type="list" 
					default="0"
					label="CountUp"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="jquerywaypoints-site"
					 type="list" 
					default="0"
					label="Jquery Waypoints"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="isotope-site"
					 type="list" 
					default="0"
					label="Jquery isotope"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="venobox-site"
					 type="list" 
					default="0"
					label="venobox"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="owlcarousel-site"
					 type="list" 
					default="0"
					label="OwlCarousel"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
				<field 
					name="aos-site"
					 type="list" 
					default="0"
					label="aos"
					description="Version">
						<option value="0">JFIELD_MOD_OPENSOURCE_HIDE</option>
						<option value="1">JFIELD_MOD_OPENSOURCE_SHOW</option>
				</field>
			</fieldset>
			<fieldset name="version" label="Version">	
				<field 
					name="fontawesome-version"
					 type="text"
					default="5.14.0"
					label="font awesome" 
					description=""
					filter="string" 
				/>
				<field 
					name="jquery-version"
					 type="text"
					default="3.5.1"
					label="Jquery" 
					description=""
					filter="string" 
				/>
				<field 
					name="jqueryeasingjs-version"
					 type="text" 
					default="1.4.1"
					label="jquery easing" 
					description=""
					filter="string" 
				/>
				<field 
					name="bootstrap-version"
					 type="text"
					default="4.5.0"
					label="Bootstrap" 
					description=""
					filter="string" 
				/>
				
				<field 
					name="animate-version"
					 type="text"
					default="4.1.0"
					label="Animate.css" 
					description=""
					filter="string" 
				/>
				<field 
					name="wowjs-version"
					 type="text"
					default="1.1.2"
					label="wow.js" 
					description=""
					filter="string" 
				/>
				<field 
					name="countupjs-version"
					 type="text"
					default="2.0.6"
					label="CountUp" 
					description=""
					filter="string" 
				/>
				<field 
					name="jquerywaypoints-version"
					 type="text"
					default="4.0.1"
					label="Jquery Waypoints" 
					description=""
					filter="string" 
				/>
				<field 
					name="isotope-version"
					 type="text"
					default="3.0.6"
					label="jquery isotope" 
					description=""
					filter="string" 
				/>
				<field 
					name="venobox-version"
					 type="text"
					default="1.9.0"
					label="venobox" 
					description=""
					filter="string" 
				/>
				<field 
					name="owlcarousel-version"
					 type="text"
					default="2.3.4"
					label="owlcarousel" 
					description=""
					filter="string" 
				/>
				<field 
					name="aos-version"
					 type="text"
					default="2.3.4"
					label="aos" 
					description=""
					filter="string" 
				/>
			</fieldset>
			<fieldset name="templates" label="JFIELD_MOD_OPENSOURCE_FIELDSET_LABEL">	
				<field
					name="head-script"
					type="textarea"
					label="JFIELD_MOD_OPENSOURCE_HEAD_LABEL"
					description="JFIELD_MOD_OPENSOURCE_HEAD_DESCRIPTION"
					rows="3"
				/>
				<field
					name="body-script"
					type="textarea"
					label="JFIELD_MOD_OPENSOURCE_BODY_LABEL"
					description="JFIELD_MOD_OPENSOURCE_BODY_DESCRIPTION"
					rows="3"
				/>
			</fieldset>
		</fields>
	</config>
</extension>
